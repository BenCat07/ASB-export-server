{
	"name": "asb-export-server",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"start": "node build",
		"docker:build": "docker buildx build --platform=linux/amd64,linux/arm64/v8 -t coldino/asb-export-server --push .",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:unit": "vitest",
		"test:e2e-watch": "PWTEST_WATCH=1 playwright test",
		"test:e2e": "playwright test",
		"test": "npm run test:unit -- --run && npm run test:e2e"
	},
	"devDependencies": {
		"@playwright/test": "^1.49.1",
		"@sveltejs/adapter-node": "^5.0",
		"@sveltejs/enhanced-img": "^0.1.9",
		"@sveltejs/kit": "^2.5.5",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.4",
		"@types/node": "^22.13.1",
		"@typescript-eslint/eslint-plugin": "*",
		"@typescript-eslint/parser": "*",
		"@unocss/extractor-svelte": "^0.59",
		"@unocss/preset-uno": "^0.59",
		"@unocss/reset": "^0.59",
		"@unocss/transformer-directives": "^0.59",
		"eslint": "^8.56",
		"eslint-config-prettier": "*",
		"eslint-plugin-svelte": "*",
		"jsdom": "^25.0.1",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "*",
		"svelte": "^4.2.13",
		"svelte-check": "*",
		"sveltekit-rate-limiter": "^0.5.1",
		"tslib": "*",
		"typescript": "^5.4.5",
		"undici": "^7.3.0",
		"unocss": "^0.59",
		"vite": "^5",
		"vitest": "^3.0.0"
	},
	"type": "module",
	"engines": {
		"node": ">=20.18.1"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
